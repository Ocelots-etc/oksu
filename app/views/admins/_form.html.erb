  <%= form_for @admin  do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br />

    <%= f.label :email %>
    <%= f.text_field :email %>
    <br />

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br />
  <% if !current_admin %>
    <%= f.submit "Create Admin Account" %>
    <% end %>
  <% if current_admin %>
    <%= f.submit "Submit Edit Form" %>
    <% end %>
    
  <% end %>
<br />
<br />
<br />
  <% if current_admin %>

   <%= button_to "Edit Your Admin Account", edit_admin_path(current_admin), method: :get %>
 <%= button_to "Delete Your Admin Account", admin_path(current_admin), method: :DELETE %>
<% end %>