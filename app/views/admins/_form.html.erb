  <%= form_for @admin  do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br />

    <%= f.label :email %>
    <%= f.text_field :email %>
    <br />

    <%= f.label :password %>
    <%= f.password_field :password %>
    <br />

    <%= f.submit "Create Admin Account" %>
  <% end %>